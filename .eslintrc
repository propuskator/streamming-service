{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "webbylab"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-magic-numbers": 0,
        "import/named": 0
    }
}